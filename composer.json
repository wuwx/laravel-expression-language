{
    "name": "wuwx/laravel-expression-language",
    "type": "library",
    "keywords": ["laravel", "expression", "language", "expression-language", "ExpressionLanguage"],
    "license": "MIT",
    "authors": [
        {
            "name": "Wu Weixin",
            "email": "wuweixin@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": false
    },
    "require": {
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0",
        "laravel/helpers": "^1.0",
        "symfony/expression-language": "^3.3|^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "^3.6|^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Wuwx\\LaravelExpressionLanguage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wuwx\\LaravelExpressionLanguage\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wuwx\\LaravelExpressionLanguage\\Providers\\ExpressionLanguageServiceProvider"
            ],
            "aliases": {
                "ExpressionLanguage": "Wuwx\\LaravelExpressionLanguage\\Facades\\ExpressionLanguageFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
